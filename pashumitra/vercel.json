{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/injection-notifications",
      "schedule": "*/15 * * * *"
    }
  ],
  "functions": {
    "api/cron/injection-notifications.js": {
      "runtime": "nodejs18.x"
    }
  }
}